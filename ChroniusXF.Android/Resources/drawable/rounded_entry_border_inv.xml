<?xml version="1.0" encoding="UTF-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="0dp"
                android:color="#000000" />
            <solid
                android:color="#FFF" />
            <padding
                android:top="0dp"
                android:left="20dp"
                android:bottom="0dp"
                android:right="0dp" />
            <corners
                android:radius="20dp" />
        </shape>
    </item>
</selector>